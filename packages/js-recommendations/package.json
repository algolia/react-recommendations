{
  "name": "@algolia/js-recommendations",
  "version": "1.0.0-beta.9",
  "license": "MIT",
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:esm": "cross-env BUILD=esm rollup --config",
    "build:umd": "cross-env BUILD=umd rollup --config",
    "build:types": "tsc -p ./tsconfig.declaration.json --outDir ./dist/esm",
    "build": "yarn build:clean && yarn build:esm && yarn build:umd && yarn build:types",
    "prepare": "yarn build:esm && yarn build:types"
  },
  "source": "src/index.ts",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/umd/index.js",
  "umd:main": "dist/umd/index.js",
  "unpkg": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "algoliasearch": "^4.9.1"
  },
  "dependencies": {
    "@algolia/react-recommendations": "1.0.0-beta.9",
    "preact": "^10.0.0"
  }
}
